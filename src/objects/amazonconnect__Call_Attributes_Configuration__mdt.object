<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Custom metadata object to define which call attributes should be displayed in Amazon&apos;s CCP and what labels to use for them.</description>
    <fields>
        <fullName>amazonconnect__Attribute_Name__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <description>The name of the attribute as it is configured in Amazon Connect.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Please provide the name of the attribute as it is configured in Amazon Connect&apos;s Call Flow.</inlineHelpText>
        <label>Amazon Connect Attribute Name</label>
        <length>50</length>
        <required>true</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>amazonconnect__CCP_Attribute_Label__c</fullName>
        <deprecated>false</deprecated>
        <description>The label to be used in the CCP display.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Please provide the label for this attribute as it will be shown in the display within CCP. Ideally, the shorter the name the better it is.</inlineHelpText>
        <label>CCP Attribute Label</label>
        <length>25</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Call Attributes Configuration</label>
    <pluralLabel>Call Attributes Configurations</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
